<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <ProgressBar
        android:id="@+id/progressBar"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerInParent="true"
        android:indeterminate="true" />

    <RelativeLayout
        android:id="@+id/container"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <ScrollView
            android:id="@+id/scroll_view"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_above="@+id/lin_bottom">

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <ImageView
                    android:id="@+id/img_congrats"
                    android:layout_width="98dp"
                    android:layout_height="98dp"
                    android:layout_alignParentTop="true"
                    android:layout_centerHorizontal="true"
                    android:layout_margin="@dimen/padding_far"
                    android:src="@drawable/image_check2" />

                <TextView
                    android:id="@+id/txt_congrats"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/img_congrats"
                    android:layout_centerHorizontal="true"
                    android:layout_marginBottom="@dimen/padding_standard_half"
                    android:gravity="center_horizontal"
                    android:text="@string/vor_congrats"
                    android:textSize="24sp"
                    android:textStyle="bold"
                    android:paddingLeft="@dimen/padding_standard"
                    android:paddingRight="@dimen/padding_standard"/>

                <TextView
                    android:id="@+id/txt_congrats_info"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/txt_congrats"
                    android:layout_centerHorizontal="true"
                    android:text="@string/vor_congrats_info"
                    android:textColor="@android:color/darker_gray" />

                <RelativeLayout
                    android:id="@+id/rel_event_head"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_below="@id/txt_congrats_info"
                    android:layout_marginBottom="@dimen/padding_far"
                    android:layout_marginLeft="@dimen/padding_standard"
                    android:layout_marginRight="@dimen/padding_standard"
                    android:layout_marginTop="@dimen/padding_far"
                    android:background="@drawable/shape_dash">

                    <TextView
                        android:id="@+id/txt_event_title"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentTop="true"
                        android:layout_centerHorizontal="true"
                        android:layout_marginBottom="@dimen/padding_standard_half"
                        android:layout_marginTop="@dimen/padding_standard"
                        android:text="@string/vor_event_title"
                        android:textAllCaps="true"
                        android:textColor="@color/purple"
                        android:textSize="14sp"
                        android:textStyle="bold" />

                    <TextView
                        android:id="@+id/txt_event_date"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_below="@+id/txt_event_title"
                        android:layout_centerHorizontal="true"
                        android:paddingBottom="@dimen/padding_standard"
                        android:text="@string/vor_event_date"
                        android:textColor="@android:color/darker_gray" />
                </RelativeLayout>

                <TextView
                    android:id="@+id/txt_type_number_title"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentLeft="true"
                    android:layout_below="@+id/rel_event_head"
                    android:layout_margin="@dimen/padding_standard"
                    android:text="@string/vor_reservation_number"
                    android:textSize="16sp"
                    android:textStyle="bold"
                    android:textAllCaps="true"/>

                <TextView
                    android:id="@+id/txt_type_number_fill"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:layout_below="@+id/rel_event_head"
                    android:layout_margin="@dimen/padding_standard"
                    android:text="@string/vor_order_number_fill"
                    android:textAllCaps="true"
                    android:textColor="@color/purple"
                    android:textSize="16sp"
                    android:textStyle="bold" />

                <View
                    android:id="@+id/divider1"
                    android:layout_width="match_parent"
                    android:layout_height="0.5dp"
                    android:layout_below="@id/txt_type_number_fill"
                    android:layout_marginLeft="@dimen/padding_standard"
                    android:layout_marginRight="@dimen/padding_standard"
                    android:background="@android:color/darker_gray" />

                <LinearLayout
                    android:id="@+id/lin_order_number"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_below="@id/divider1"
                    android:orientation="vertical">

                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_margin="@dimen/padding_standard">

                        <TextView
                            android:id="@+id/txt_guest_name_title"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignParentLeft="true"
                            android:text="@string/vor_guest_name"
                            android:textSize="@dimen/normal_text_size" />

                        <TextView
                            android:id="@+id/txt_guest_name_fill"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignParentRight="true"
                            android:text="@string/vor_guest_name_fill"
                            android:textAllCaps="true"
                            android:textColor="@color/purple"
                            android:textSize="@dimen/normal_text_size" />
                    </RelativeLayout>

                    <RelativeLayout
                        android:id="@+id/container_table_guest"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/padding_standard"
                        android:layout_marginLeft="@dimen/padding_standard"
                        android:layout_marginRight="@dimen/padding_standard"
                        android:visibility="gone">

                        <TextView
                            android:id="@+id/txt_guest_count_title"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignParentLeft="true"
                            android:text="@string/vor_total_guest"
                            android:textSize="@dimen/normal_text_size" />

                        <TextView
                            android:id="@+id/txt_guest_count_fill"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignParentRight="true"
                            android:text="@string/vor_guest_name_fill"
                            android:textAllCaps="true"
                            android:textColor="@color/purple"
                            android:textSize="@dimen/normal_text_size" />
                    </RelativeLayout>

                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/padding_standard"
                        android:layout_marginLeft="@dimen/padding_standard"
                        android:layout_marginRight="@dimen/padding_standard">

                        <TextView
                            android:id="@+id/txt_status_title"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignParentLeft="true"
                            android:text="@string/vor_status"
                            android:textSize="@dimen/normal_text_size" />

                        <TextView
                            android:id="@+id/txt_status_fill"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignParentRight="true"
                            android:text="@string/vor_status_paid"
                            android:textAllCaps="true"
                            android:textColor="@color/purple"
                            android:textSize="@dimen/normal_text_size" />
                    </RelativeLayout>

                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/padding_standard"
                        android:layout_marginLeft="@dimen/padding_standard"
                        android:layout_marginRight="@dimen/padding_standard">

                        <TextView
                            android:id="@+id/txt_payment_title"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignParentLeft="true"
                            android:text="@string/vor_payment_method"
                            android:textSize="@dimen/normal_text_size" />

                        <TextView
                            android:id="@+id/txt_payment_fill"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignParentRight="true"
                            android:text="@string/vor_payment_method_fill"
                            android:textColor="@color/purple"
                            android:textSize="@dimen/normal_text_size"
                            android:textAllCaps="true"/>
                    </RelativeLayout>
                </LinearLayout>


                <TextView
                    android:id="@+id/txt_summary_title"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentLeft="true"
                    android:layout_below="@+id/lin_order_number"
                    android:layout_marginBottom="@dimen/padding_standard_half"
                    android:layout_marginLeft="@dimen/padding_standard"
                    android:layout_marginRight="@dimen/padding_standard"
                    android:layout_marginTop="@dimen/padding_standard"
                    android:text="@string/vor_order_summary"
                    android:textSize="16sp"
                    android:textStyle="bold" />

                <View
                    android:id="@+id/divider2"
                    android:layout_width="match_parent"
                    android:layout_height="0.5dp"
                    android:layout_alignParentRight="true"
                    android:layout_below="@id/lin_order_number"
                    android:layout_margin="@dimen/padding_far"
                    android:layout_toRightOf="@id/txt_summary_title"
                    android:background="@android:color/darker_gray" />

                <TextView
                    android:id="@+id/txt_summary_date"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentLeft="true"
                    android:layout_below="@+id/txt_summary_title"
                    android:layout_marginBottom="@dimen/padding_far"
                    android:layout_marginLeft="@dimen/padding_standard"
                    android:text="@string/vor_order_summary_date"
                    android:textSize="@dimen/normal_text_size" />

                <LinearLayout
                    android:id="@+id/lin_summary"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_below="@id/txt_summary_date"
                    android:orientation="vertical">

                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/padding_standard"
                        android:layout_marginLeft="@dimen/padding_standard"
                        android:layout_marginRight="@dimen/padding_standard">

                        <TextView
                            android:id="@+id/txt_reg_ticket_title"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignParentLeft="true"
                            android:text="@string/vor_reg_ticket"
                            android:textSize="@dimen/normal_text_size" />

                        <TextView
                            android:id="@+id/txt_reg_ticket_fill"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignParentRight="true"
                            android:text="@string/vor_reg_ticket_fill"
                            android:textColor="@color/purple"
                            android:textSize="@dimen/normal_text_size" />
                    </RelativeLayout>

                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/padding_standard"
                        android:layout_marginLeft="@dimen/padding_standard"
                        android:layout_marginRight="@dimen/padding_standard">

                        <TextView
                            android:id="@+id/txt_ad_fee_title"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignParentLeft="true"
                            android:text="@string/vor_ad_fee"
                            android:textSize="@dimen/normal_text_size" />

                        <TextView
                            android:id="@+id/txt_ad_fee_fill"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignParentRight="true"
                            android:text="@string/vor_ad_fee_fill"
                            android:textColor="@color/purple"
                            android:textSize="@dimen/normal_text_size" />
                    </RelativeLayout>

                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/padding_standard"
                        android:layout_marginLeft="@dimen/padding_standard"
                        android:layout_marginRight="@dimen/padding_standard">

                        <TextView
                            android:id="@+id/txt_tax_title"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignParentLeft="true"
                            android:text="@string/vor_tax"
                            android:textSize="@dimen/normal_text_size" />

                        <TextView
                            android:id="@+id/txt_tax_fill"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignParentRight="true"
                            android:text="@string/vor_tax_fill"
                            android:textColor="@color/purple"
                            android:textSize="@dimen/normal_text_size" />
                    </RelativeLayout>
                </LinearLayout>

                <View
                    android:id="@+id/divider8"
                    android:layout_width="match_parent"
                    android:layout_height="0.5dp"
                    android:layout_alignParentRight="true"
                    android:layout_below="@+id/lin_summary"
                    android:layout_marginBottom="@dimen/padding_standard"
                    android:layout_marginLeft="@dimen/padding_standard"
                    android:layout_marginRight="@dimen/padding_standard"
                    android:layout_toRightOf="@+id/txt_summary_title"
                    android:background="@android:color/darker_gray" />

                <LinearLayout
                    android:id="@+id/lin_summary_footer"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/divider8"
                    android:orientation="vertical">

                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/padding_standard"
                        android:layout_marginLeft="@dimen/padding_standard"
                        android:layout_marginRight="@dimen/padding_standard">

                        <TextView
                            android:id="@+id/lbl_estimate_total_title"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignParentLeft="true"
                            android:text="@string/estimated_total"
                            android:textSize="@dimen/normal_text_size" />

                        <TextView
                            android:id="@+id/lbl_estimate_ttal_fill"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignParentRight="true"
                            android:text="@string/vor_tax_fill"
                            android:textColor="@color/purple"
                            android:textSize="@dimen/normal_text_size" />
                    </RelativeLayout>

                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/padding_standard"
                        android:layout_marginLeft="@dimen/padding_standard"
                        android:layout_marginRight="@dimen/padding_standard">

                        <TextView
                            android:id="@+id/lbl_required_deposit_title"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignParentLeft="true"
                            android:text="@string/paid_deposit"
                            android:textSize="@dimen/normal_text_size" />

                        <TextView
                            android:id="@+id/lbl_required_deposit_fill"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignParentRight="true"
                            android:text="@string/vor_tax_fill"
                            android:textColor="@color/green_material"
                            android:textSize="@dimen/normal_text_size"
                            android:textStyle="bold"/>
                    </RelativeLayout>

                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/padding_standard"
                        android:layout_marginLeft="@dimen/padding_standard"
                        android:layout_marginRight="@dimen/padding_standard">

                        <TextView
                            android:id="@+id/lbl_estimated_balance_title"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignParentLeft="true"
                            android:text="@string/estimated_balance"
                            android:textSize="@dimen/normal_text_size" />

                        <TextView
                            android:id="@+id/lbl_estimated_balance_fill"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignParentRight="true"
                            android:text="@string/vor_tax_fill"
                            android:textColor="@color/purple"
                            android:textSize="@dimen/normal_text_size" />
                    </RelativeLayout>
                </LinearLayout>

                <View
                    android:id="@+id/divider3"
                    android:layout_width="match_parent"
                    android:layout_height="0.5dp"
                    android:layout_alignParentRight="true"
                    android:layout_below="@+id/lin_summary_footer"
                    android:layout_marginBottom="@dimen/padding_standard"
                    android:layout_marginLeft="@dimen/padding_standard"
                    android:layout_marginRight="@dimen/padding_standard"
                    android:layout_toRightOf="@id/txt_summary_title"
                    android:background="@android:color/darker_gray" />

                <TextView
                    android:id="@+id/txt_total"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentLeft="true"
                    android:layout_below="@+id/divider3"
                    android:layout_marginBottom="@dimen/padding_far"
                    android:layout_marginLeft="@dimen/padding_standard"
                    android:layout_marginRight="@dimen/padding_standard"
                    android:text="@string/vor_total"
                    android:textSize="16sp"
                    android:textStyle="bold" />

                <TextView
                    android:id="@+id/txt_total_fill"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:layout_below="@+id/divider3"
                    android:layout_marginBottom="@dimen/padding_far"
                    android:layout_marginLeft="@dimen/padding_standard"
                    android:layout_marginRight="@dimen/padding_standard"
                    android:text="@string/vor_total_fill"
                    android:textColor="@color/purple"
                    android:textSize="16sp"
                    android:textStyle="bold" />

                <TextView
                    android:id="@+id/txt_instruc"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentLeft="true"
                    android:layout_below="@+id/txt_total_fill"
                    android:layout_margin="@dimen/padding_standard"
                    android:text="@string/vor_instruction"
                    android:textColor="@color/purple"
                    android:textSize="16sp"
                    android:textStyle="bold" />

                <View
                    android:id="@+id/divider4"
                    android:layout_width="match_parent"
                    android:layout_height="0.5dp"
                    android:layout_below="@id/txt_total_fill"
                    android:layout_marginBottom="@dimen/padding_standard"
                    android:layout_marginLeft="@dimen/padding_standard"
                    android:layout_marginRight="@dimen/padding_standard"
                    android:layout_marginTop="@dimen/padding_far"
                    android:layout_toRightOf="@id/txt_instruc"
                    android:background="@android:color/darker_gray" />

                <TextView
                    android:id="@+id/txt_instruc_fill"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentLeft="true"
                    android:layout_below="@+id/txt_instruc"
                    android:layout_marginLeft="@dimen/padding_standard"
                    android:layout_marginRight="@dimen/padding_standard"
                    android:text="@string/vor_instruction_fill" />

                <View
                    android:id="@+id/divider5"
                    android:layout_width="match_parent"
                    android:layout_height="0.5dp"
                    android:layout_below="@id/txt_instruc_fill"
                    android:layout_marginTop="@dimen/padding_standard"
                    android:background="@android:color/darker_gray" />

                <RelativeLayout
                    android:id="@+id/rel_event"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_below="@id/divider5"
                    android:layout_margin="@dimen/padding_standard">

                    <RelativeLayout
                        android:id="@+id/img_clock"
                        android:layout_width="24dp"
                        android:layout_height="wrap_content"
                        android:layout_alignParentLeft="true"
                        android:layout_centerVertical="true"
                        android:layout_marginRight="@dimen/padding_standard">

                        <ImageView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_centerInParent="true"
                            android:src="@drawable/icon_clock2x" />
                    </RelativeLayout>

                    <TextView
                        android:id="@+id/txt_event_title2"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentTop="true"
                        android:layout_marginBottom="@dimen/padding_standard_half"
                        android:layout_toRightOf="@id/img_clock"
                        android:text="@string/vor_event_title"
                        android:textSize="14sp"
                        android:textStyle="bold" />

                    <TextView
                        android:id="@+id/txt_event_date2"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_below="@id/txt_event_title2"
                        android:layout_toRightOf="@id/img_clock"
                        android:text="@string/vor_event_date"
                        android:textSize="@dimen/normal_text_size" />
                </RelativeLayout>

                <RelativeLayout
                    android:id="@+id/rel_venue"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_below="@id/rel_event"
                    android:layout_marginBottom="@dimen/padding_standard"
                    android:layout_marginLeft="@dimen/padding_standard"
                    android:layout_marginRight="@dimen/padding_standard">

                    <RelativeLayout
                        android:id="@+id/img_map"
                        android:layout_width="24dp"
                        android:layout_height="wrap_content"
                        android:layout_alignParentLeft="true"
                        android:layout_centerVertical="true"
                        android:layout_marginRight="@dimen/padding_standard">

                        <ImageView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_centerInParent="true"
                            android:src="@drawable/icon_map2x" />
                    </RelativeLayout>

                    <TextView
                        android:id="@+id/txt_venue_title"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentTop="true"
                        android:layout_marginBottom="@dimen/padding_standard_half"
                        android:layout_toRightOf="@id/img_map"
                        android:text="@string/vor_event_title"
                        android:textSize="14sp"
                        android:textStyle="bold" />

                    <TextView
                        android:id="@+id/txt_venue_date"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_below="@id/txt_venue_title"
                        android:layout_toRightOf="@id/img_map"
                        android:text="@string/vor_event_date"
                        android:textSize="@dimen/normal_text_size" />
                </RelativeLayout>

                <View
                    android:id="@+id/divider6"
                    android:layout_width="match_parent"
                    android:layout_height="0.5dp"
                    android:layout_below="@id/rel_venue"
                    android:background="@android:color/darker_gray" />

            </RelativeLayout>
        </ScrollView>

        <LinearLayout
            android:id="@+id/lin_bottom"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_alignParentBottom="true"
            android:orientation="vertical">

            <RelativeLayout
                android:id="@+id/rel_info_tap"
                android:layout_width="match_parent"
                android:layout_height="?attr/actionBarSize">

                <TextView
                    android:id="@+id/txt_star"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="@dimen/padding_standard_half"
                    android:text="@string/howtp_star"
                    android:layout_alignParentLeft="true"
                    android:textColor="@color/purple"
                    android:layout_centerVertical="true"/>

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:text="@string/vor_tap_info"
                    android:textSize="@dimen/normal_text_size"
                    android:layout_toRightOf="@id/txt_star"/>
            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/rel_view_ticket"
                android:layout_width="match_parent"
                android:layout_height="?attr/actionBarSize"
                android:background="@color/blue_selector">

                <TextView
                    android:id="@+id/txt_view_ticket"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerInParent="true"
                    android:text="@string/howtp_vo"
                    android:textColor="@color/common_plus_signin_btn_text_dark"
                    android:textSize="16sp"/>
            </RelativeLayout>
        </LinearLayout>
    </RelativeLayout>
</RelativeLayout>